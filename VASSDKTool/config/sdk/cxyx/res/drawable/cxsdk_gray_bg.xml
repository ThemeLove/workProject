<shape xmlns:android="http://schemas.android.com/apk/res/android" >
    <!-- 图形填充颜色 -->
    <solid android:color="#ffeeeeee" />
    <!-- 边界线宽度及颜色 -->
    <stroke
        android:width="1dp"
        android:color="#ffbbbbbb" />
    <!-- 圆角大小，以圆的半径表示 -->
    <corners android:radius="5dp" />
    <!-- 图形内部边距 -->
    <padding
        android:bottom="10dp"
        android:left="10dp"
        android:right="10dp"
        android:top="10dp" />
</shape>